{
  "source": "./imports",
  "destination": "./docs",
  "includes": ["\\.(js|jsx)$"],
  "plugins": [
    {
      "name": "esdoc2-standard-plugin"
    },
    {
      "name": "esdoc2-jsx-plugin", 
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc2-ecmascript-proposal-plugin", 
      "option": {
        "all": true
      }
    }
  ]
}
