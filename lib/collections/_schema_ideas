//schema ideas
//gene
{
	_id: String, //unique
	ID: String, //unique
	seq: String,
	source: String,
	start: Number, 
	end: Number,
	strand: String,
	trackName: String,
	attributes: [
		{
			key: value
		}
	],
	subfeatures: [
		{
			ID: String, //unique
			type: String, //CDS, mRNA, etc.
			start: Number,
			end: Number,
			score: Number,
			parents: [String], //other subfeature IDs
			children: [String] //other subfeature IDs
			attributes: [
				{
					key: value
				}
			]
		}
	],
	expression : [
		{
			experiment_id: String,
			tpm: Number,
			raw_count: Number
		}
	]
}

//experiment
{
	_id: String, //unique
	ID: String,
	description: String,
	permissions: [String]
}

