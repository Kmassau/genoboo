{
  "name": "genenotebook",
  "version": "0.1.9",
  "repository": "https://github.com/genenotebook/genenotebook",
  "description": "A portable website for browsing and querying genome sequences and annotations",
  "license": "AGPL-3.0",
  "scripts": {
    "preinstall": "meteor help || curl https://install.meteor.com/ | sh",
    "preupdate": "meteor update",
    "docs": "documentation build --shallow imports/** -f html -o api-docs",
    "postdocs": "cp api-docs/index.html docs/pages/api-docs.html && cp -r api-docs/assets docs/",
    "bundle": "./bundle.sh",
    "complete_build": "npm install && npm run bundle",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 meteor run",
    "dev_production": "NODE_OPTIONS=--max-old-space-size=8192 meteor run --production",
    "reset": "meteor reset"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "asteroid": "^2.0.3",
    "bcrypt": "^3.0.2",
    "bootstrap": "^4.1.2",
    "buffer-shims": "^1.0.0",
    "bufferutil": "^4.0.0",
    "child-process": "^1.0.2",
    "color": "^3.1.0",
    "commander": "^2.19.0",
    "core-js": "^2.5.7",
    "core-util-is": "^1.0.2",
    "d3": "^5.7.0",
    "documentation": "^8.1.2",
    "dot-object": "^1.7.1",
    "fibers": "^3.0.0",
    "file-type": "^10.4.0",
    "fs.extra": "^1.3.2",
    "glob": "^7.1.3",
    "html-entities": "^1.2.1",
    "immutability-helper": "^2.8.1",
    "install": "^0.12.2",
    "lodash": "^4.17.11",
    "meteor-babel-helpers": "0.0.3",
    "meteor-job": "^1.5.2",
    "meteor-later": "^1.2.0",
    "meteor-node-stubs": "^0.4.0",
    "object-hash": "^1.3.0",
    "papaparse": "^4.6.1",
    "randomcolor": "^0.5.3",
    "react": "^16.6.1",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.6.1",
    "react-mounter": "^1.2.0",
    "react-resize-detector": "^3.2.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.1",
    "reactstrap": "^6.5.0",
    "readline-promise": "^1.0.3",
    "recompose": "^0.30.0",
    "rus-diff": "^1.1.0",
    "simpl-schema": "^1.5.3",
    "spawn-promise": "^0.1.8",
    "spawn-sync": "^2.0.0",
    "thread-sleep": "^2.1.0",
    "try-thread-sleep": "^2.0.0",
    "utf-8-validate": "^5.0.0",
    "webfontloader": "^1.6.28",
    "ws": "^6.1.0",
    "xml2js-es6-promise": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  }
}
